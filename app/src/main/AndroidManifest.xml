<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.test.imageload">

    <uses-permission android:name="android.permission.INTERNET" />

    <!--android:usesCleartextTraffic="true" sdk =29 允许http -->
    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">

        <activity android:name=".CusZoomViewActivity" />

        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".kotlin.MainActivity" />
        <activity android:name="com.test.imageload.picture.lifecycle.LifeCycleActivity" />
        <activity android:name=".kotlin.ImageLoadLongPicActivity" />
        <activity android:name=".kotlin.BrowPicActivity" />
        <activity android:name=".kotlin.HomeActivity" />
        <activity
            android:name=".kotlin.DetailActivity"
            android:theme="@style/DetailTheme" />
        <activity android:name=".kotlin.VideoCacheActivity" />
    </application>

</manifest>